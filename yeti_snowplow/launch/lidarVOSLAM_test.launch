<launch>
	<!-- LiDAR -->
	<arg name="host" default="192.168.0.100" />
	<node pkg="isc_sick" name="isc_sick" type="LMS1xx_node">
		<param name="host" value="$(arg host)" />
		<param name="frame_id" value="sick_laser_link" />
		<param name="use_tf_correction" value="true" />
	</node>
	<node pkg="zenith_localization" name="zenith_localization" type="zenith_localizer">
</node>
<node pkg="pose_publisher" name="pose_publisher" type="pose_publisher_node">
</node>
	<include file="$(find yeti_snowplow)/launch/zed.launch">
    	<arg name="publish_map_tf" value="false" />
   		<arg name="base_frame" value="base_link" />
	</include>
	<node pkg="tf" type="static_transform_publisher" name="static_transform_publisher_zed" args="0 0 0 0 0 0 base_link zed_camera_center 10" />
	<node pkg="tf" type="static_transform_publisher" name="static_transform_publisher_sick_laser_link" args="0 0 0 0 0 0 base_link sick_laser_link 10" />

	<node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="screen">
		<param name="map_update_interval" value="2.0" />
		<!-- <param name="base_frame" value="zed_camera_center" /> -->
	</node>
</launch>
